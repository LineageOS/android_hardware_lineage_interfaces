        "powerhal_mode_ext_defaults",
    ],
    relative_install_path: "hw",
    init_rc: ["android.hardware.power-service.libperfmgr.rc"],
    vintf_fragments: ["android.hardware.power-service.xml"],
    vendor: true,
    shared_libs: [
        "android.hardware.power-V3-ndk",
        "libadaptivecpu",
        "libbase",
        "libcutils",
        "liblog",
        "libutils",
        "libbinder_ndk",
        "libperfmgr",
